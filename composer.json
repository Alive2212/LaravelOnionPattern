{
    "name": "alive2212/laravel-onion-pattern",
    "description": "This is a pattern for implementation like onion",
    "license": "MIT",
    "authors": [
        {
            "name": "Babak Nodoust",
            "email": "alive2212@yahoo.com",
            "homepage": "http://babakn.com"
        }
    ],
    "homepage": "https://github.com/alive2212/laravelonionpattern",
    "keywords": ["Laravel", "LaravelOnionPattern"],
    "require": {
        "illuminate/support": "~7"
    },
    "require-dev": {
        "phpunit/phpunit": "~7",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Alive2212\\LaravelOnionPattern\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alive2212\\LaravelOnionPattern\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alive2212\\LaravelOnionPattern\\LaravelOnionPatternServiceProvider"
            ],
            "aliases": {
                "LaravelOnionPattern": "Alive2212\\LaravelOnionPattern\\Facades\\LaravelOnionPattern"
            }
        }
    }
}